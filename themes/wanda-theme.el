(require 'define-theme)

(define-theme wanda
  :background "#1B122B"
  :foreground "#E8F0DB"
  :error "#FF6666"
  :warning "#E6A219"
  :success "#36CF17"
  :info "#99FFFF"
  :keyword "#E63737"
  :type "#E6AC37"
  :constant "#7BAD1F")

(provide-theme 'wanda)
